<%= semantic_form_for [ 'admin', institution ] do |form| -%>
  <%= form.inputs :admin_institution do %>
    <%= form.input :code %>
    <%= form.input :name %>
  <% end %>
  
  <%= form.buttons do %>
    <%= redirect_field %>
    <%= form.commit_button %>
    <%= form_cancel_link admin_institutions_path %>
  <% end %>
<% end -%>
