<% @title = t('views.contributor.guests.edit.title') %>

<%= semantic_form_for @contact, :url => contributor_guest_path do |form| %>
  <%= render :partial => '/contacts/form_fields', :locals => { :form => form } %>
  <%= form.buttons do %>
    <%= redirect_field contributor_dashboard_path %>
    <%= form.commit_button %>
    <%= form_cancel_link contributor_dashboard_path %>
  <% end %>
<% end %>
