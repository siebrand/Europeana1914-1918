<%= theme_stylesheet_link_tag 'application', :media => 'all' %>
<%= theme_stylesheet_link_tag 'application-screen', :media => 'screen, projection' %>
<% if @javascripts[:uploadify] -%>
  <%= theme_stylesheet_link_tag 'uploadify', :media => 'screen, projection' %>
<% end -%>
<% if @javascripts[:video] || @javascripts[:audio] -%>
  <%= stylesheet_link_tag 'mediaelementplayer', :media => 'screen, projection' %>
<% end -%>
<% if @javascripts[:annotorious] -%>
  <%= stylesheet_link_tag 'annotorious', :media => 'screen, projection' %>
<% end -%>
<% if @javascripts[:page] == 'index' -%>
  <%= theme_stylesheet_link_tag "index" %>
<% end -%>
<% if @javascripts[:page] == 'search' -%>
  <%= theme_stylesheet_link_tag "search" %>
<% end -%>
<!--[if lt IE 9]><%= theme_stylesheet_link_tag "ie", :media => 'all' %><![endif]-->
<noscript><%= theme_stylesheet_link_tag "noscript", :media => 'all' %></noscript>
<%= theme_javascript_include_tag 'respond' %>
